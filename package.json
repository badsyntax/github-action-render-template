{
  "name": "github-action-render-template",
  "version": "0.0.0",
  "private": true,
  "description": "A GitHub Action to render a handlebars template",
  "main": "lib/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write '**/*.{ts,json,svg,md,yml}'",
    "format-check": "prettier --check '**/*.{ts,json,svg,md,yml}'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/main.js --source-map --license licenses.txt",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badsyntax/github-action-render-template.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "aws",
    "cloudfront"
  ],
  "author": "Richard Willis <willis.rh@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@typescript-eslint/parser": "^5.12.1",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.2"
  }
}
